import java.util.Random;


public class MultiRadixPar {

	int n;
	int [] a;
	int max;
	int index = 1;
	int numThreads;
	final static int NUM_BIT =7; // alle tall 6-11 OK

	public static void main(String [] args) {
		if (args.length != 2) {
	     	System.out.println(" bruk : >java ParallellRadix <n> <n> ");
		} else {
			int n = Integer.parseInt(args[0]);
			new MultiRadixPar().doIt(n);
		}
	} // end main

	void doIt (int len) {
		a = new int[len];
		Random r = new Random(123);
		for (int i =0; i < len;i++) {
		   a[i] = r.nextInt(len);
	    }
	    a = radixMulti(a);
	} // end doIt

	private int[] radixMulti(int[] a2) {
		max = a[0];
		//Starting with a)
		
		
		
		return null;
	}
	synchronized int getNext(){
		n = a.length;
		index++;
		if(index<n){
			return a[index];
		}	index = 1;
		return -1;
	}
	boolean biggerThanMax(int i){
		if(i>max){
			return true;
		}
		return false;
	}
	synchronized void setMax(int i){
		max = i;
	}
	
}
